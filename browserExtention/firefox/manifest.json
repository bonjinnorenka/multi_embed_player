{
	"description": "Demonstrating webRequests",
	"manifest_version": 2,
	"name": "webRequest-demo",
	"version": "1.0",
  
	"permissions": [
	  "webRequest",
	  "webRequestBlocking",
	  "*://player.bilibili.com/",
	  "*://js.ryokuryu.com/*"
	],

	"background": {
	  "scripts": ["background.js"]
	},
	"web_accessible_resources":["extention.json","liteplayer.js","player-selector.js"]
  }
  